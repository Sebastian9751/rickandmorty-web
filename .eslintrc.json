{
	"env": { "browser": true, "es2020": true },
	"extends": [
		"plugin:react/recomended",
		"standard-with-typescript",
		"plugin:prettier/recomended"
	],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module",
		"project": "./tsconfig.json"
	},
	"plugins": [
		"react-refresh",
		"prettier",
		"jsx-a11y",
		"react-hoosk",
		"@typescript-eslint"
	],
	"rules": {
		"react-refresh/only-export-components": "warn"
	}
}
